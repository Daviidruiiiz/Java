public class Program {

    /**
     *Gets the minimum number between two real numbers
     * 
     * @param a num
     * @param b num2
     *
     * @return the minimum number
     */
public int min2(int num, int num2) {
    // Variable declaration
    int awnser = num;
        if (num < num2)
                awnser = num; // If num its less than num2, change awnser with num
        else
                awnser = num2; // If num2 its less than num, change awnser with num2
    // Return the result 
        return awnser;
    }
}
